{
    "name": "abumosaab/laravel-mpdf",
    "description": "Laravel Mpdf: Using Mpdf in Laravel to generate Pdfs.",
    "keywords": ["mpdf", "pdf", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "mpdf/mpdf": "^8.0.10"
    },
    "autoload": {
        "psr-4": {"Abumosaab\\LaravelMpdf\\": "src/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abumosaab\\LaravelMpdf\\LaravelMpdfServiceProvider"
            ],
            "aliases": {
                "PDF": "Abumosaab\\LaravelMpdf\\Facades\\LaravelMpdf"
            }
        }
    }
}
